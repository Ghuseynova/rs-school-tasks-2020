{
  "env": {
    "browser": true,
    "es2020": true,
    "commonjs": true
  },
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": [],
  "rules": {
    "quotes": [
      "error",
      "single"
    ],
    "no-underscore-dangle": "off",
    "no-plusplus": [
      "error",
      {
        "allowForLoopAfterthoughts": true
      }
    ]
  },
  "ignorePatterns": [
    "node_modules/",
    "build/**/*.js",
    "webpack.common.js",
    "webpack.dev.js",
    "webpack.prod.js"
  ],
  "settings": {}
}
